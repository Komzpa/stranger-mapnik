line|z13- [highway=motorway]::mark, line|z14- [highway=trunk]::mark,
    { color: #ffffff; width: .5; z-index: 24; dashes: 7,4; z-index: 24; smooth: 0.3; }

line|z15- [highway=primary]::mark, line|z17- [highway=secondary]::mark
    { color: #ffffff; width: .5; z-index: 24; dashes: 5,3; z-index: 24; smooth: 0.3; }

/* Прочие дороги: жилые, севисные, неклассифицируемые */

/* неклассифицируемые */


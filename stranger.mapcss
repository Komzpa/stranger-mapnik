    smooth: 0.3; }

line|z14-15 [highway=primary], line|z17- [highway=primary_link]
    { color: #f59032; width: 6.5; text-color: black; text-position: line; text: cut_name; text-halo-color: #f59032;
    casing-width: 1; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 22;
    smooth: 0.3; }

line|z12-13 [highway=primary], line|z16 [highway=primary_link]
    { color: #f59032; width: 4; text-color: black; text-position: line; text: cut_name; text-halo-color: #f59032;
    casing-width: 1; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 22;
    smooth: 0.3; }

line|z10-11 [highway=primary], line|z14-15 [highway=primary_link]
    { color: #f59032; width: 3.2; text-color: black; text-position: line; text: ref; text-halo-color: #f59032;
    casing-width: 1; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 22;
    smooth: 0.3; }

line|z8-9 [highway=primary], line|z12-13 [highway=primary_link]  
    { color: #f59032; width: 2.5; text-color: black; text-position: line; text: ref; text-halo-color: #f59032;
    font-size:8; casing-width: .8; casing-color: #3d1607; text-offset: 0; text-halo-radius:1; z-index: 22;
    smooth: 0.2; }

line|z7 [highway=primary] 
    { color: #f59032; width: 1; casing-width: .4; casing-color: #3d1607; z-index: 22; }

line|z6 [highway=primary]
    { color: #f59032; width: 0.8; casing-width: .3; casing-color: #3d1607; z-index: 22; }

area|z12- [area:highway=primary]
    { fill-color: #f59032; casing-color: #3d1607; casing-width: 1; z-index: 22;}

/* Второстепенные дороги: secondary, tertiary */


line|z7 [highway=secondary]
    { color: #63310c; width: 0.75; z-index: 20; }

area|z14- [area:highway=secondary]
    { fill-color: #f0974b; casing-width: 1; casing-color: #63310c;  z-index: 20; }

line|z18- [highway=tertiary]
    { color: #ec9f62; width: 7; text-color: black; text-position: line; text: cut_name;  text-halo-color: #ec9f62;
    font-size:12; casing-width: 1; casing-color: #544900; text-offset: 0; text-halo-radius:2; z-index: 19;
   smooth: 0.3; }

line|z17 [highway=tertiary]

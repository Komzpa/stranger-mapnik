    smooth: 0.2; }

 /* смотри так же «Прочие не статусные» для unclassified на обзорных зумах */
    
area|z13- [area:highway=unclassified]
    { fill-color: #dedede; casing-width: 1; casing-color: #777; z-index: 18; }


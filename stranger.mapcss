/*
  MapCSS styles
  Stranger style
  v0.6.1.0 
*/

canvas {fill-color: #ededeb;}

area|z9- [place=allotments], way|z9- [place=hamlet],
area|z9- [place=suburb], area|z9- [place=village],
area|z8- [place=city], area|z8- [place=town]
  {fill-position: background; fill-color: #d6d6b4; fill-opacity: .45; color: #808080;
  -x-kot-layer: bottom;}
  
area|z11- [natural=coastline] {fill-color: #507dbf; fill-opacity: 1; z-index: 1;}
area|z0-10 [natural=coastline] {fill-color: #7297cc; fill-opacity: 1; z-index: 1;}

/* Основные дороги: motorway, trunk, primary */

line|z18- [highway=motorway]
    { color: #fa8819; width: 15; text-color: black; text-position: line; text: cut_name;
    font-size:14; text-halo-color: #fa8819; smooth: 0.4;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z17 [highway=motorway], line|z18 [highway=trunk]
    { color: #fa8819; width: 13.5; text-color: black; text-position: line; text: cut_name;
    font-size:13; text-halo-color: #fa8819; smooth: 0.4;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z16 [highway=motorway], line|z17 [highway=trunk]
    { color: #fa8819; width: 12; text-color: black; text-position: line; text: cut_name;
    font-size:12; text-halo-color: #fa8819; smooth: 0.4;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z14-15 [highway=motorway], line|z16 [highway=trunk],
line|z17- [highway=motorway_link]
    { color: #fa8819; width: 11; text-color: black; text-position: line; text: cut_name;
    font-size:10; text-halo-color: #fa8819; smooth: 0.4;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z12-13 [highway=motorway], line|z14-15 [highway=trunk],
line|z17- [highway=trunk_link], line|z16 [highway=motorway_link]
    { color: #fa8819; width: 8; text-color: black; text-position: line; text: cut_name;
    text-halo-color: #fa8819; smooth: 0.4;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z10-11 [highway=motorway], line|z12-13 [highway=trunk],
line|z16 [highway=trunk_link], line|z14-15 [highway=motorway_link]
    { color: #fa8819; width: 5; text-color: black; text-position: line; text: ref; text-halo-color: #fa8819;
    smooth: 0.4;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z8-9 [highway=motorway], line|z10-11 [highway=trunk],
line|z14-15 [highway=trunk_link], line|z12-13 [highway=motorway_link]
    { color: #fa8819; width: 4; text-color: black; text-position: line; text: ref; text-halo-color: #fa8819;
    smooth: 0.3;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:2; z-index: 23;}

line|z7 [highway=motorway], line|z8-9 [highway=trunk],
line|z12-13 [highway=trunk_link], line|z10-11 [highway=motorway_link]
    { color: #fa8819; width: 2.5; text-color: black; text-position: line; text: ref; text-halo-color: #fa8819;
    smooth: 0.3;
    casing-width: 1.2; casing-color: #3d1607; text-offset: 0; text-halo-radius:1; z-index: 23;}

line|z6 [highway=motorway], line|z6-7 [highway=trunk],
line|z10-11 [highway=trunk_link], line|z8-9 [highway=motorway_link]
    { color: #fa8819; width: 2; text-color: black; text-position: line; text: ref; text-halo-color: #fa8819;
    casing-width: .75; casing-color: #3d1607; text-offset: 0; text-halo-radius:1; z-index: 23;}

way|z5 [highway=motorway], line|z5 [highway=trunk]
    { color: #fa8819; width: 1; casing-width: .4; casing-color: #3d1607; z-index: 23; smooth: 0.2;}

way|z4 [highway=motorway], line|z4 [highway=trunk]
    { color: #fa8819; width: 0.8; casing-width: .3; casing-color: #3d1607; z-index: 23; smooth: 0.2;}

area|z10 [area:highway=trunk], area|z10 [area:highway=motorway]
    { fill-color: #fa8819; casing-width: 1.2; casing-color: #3d1607; z-index: 23;}

/* Главные, не магистраль */

line|z18- [highway=primary]
    { color: #f59032; width: 12; text-color: black; text-position: line; text: cut_name; font-size:13;
    text-halo-color: #f59032; casing-width: 1; casing-color: #3d1607; text-offset: 0;
    text-halo-radius:2; z-index: 22;

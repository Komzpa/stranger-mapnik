   smooth: 0.3; }

line|z10-11 [highway=tertiary]
    { color: #ec9f62; width: 1.8; text-color: black; text-position: line; text: ref; text-halo-color: #ec9f62;
    font-size:8; casing-width: .5; casing-color: #544900; text-offset: 0; text-halo-radius:2; z-index: 19;
   smooth: 0.2; }

line|z9 [highway=tertiary]
    { color: #ec9f62; width: 1; casing-width: .3; casing-color: #544900; z-index: 19; }

line|z8 [highway=tertiary]

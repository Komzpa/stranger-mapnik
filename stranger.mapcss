/* Сервисные */

line|z17- [highway=service][area!=yes], line|z18- [highway=living_street][area!=yes]
    { color: #ffffd0; width: 5; z-index: 17; casing-width: 0.5; casing-color: #a0a0a0;
    text-color: black; text-position: line; text: cut_name;  text-offset: 0; text-halo-radius:1; 

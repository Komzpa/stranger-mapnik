    { color: #f59032; width: 10; text-color: black; text-position: line; text: cut_name; font-size:12;
    text-halo-color: #f59032; casing-width: 1; casing-color: #3d1607; text-offset: 0;
    text-halo-radius:2; z-index: 22;
    smooth: 0.3; }

